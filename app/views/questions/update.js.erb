$('.question-errors').html('<%= render 'shared/errors', resource: @question %>');

<% unless @question.errors.present? %>

  $('.question-title').html('<%= @question.title %>')
  $('.question-body').html('<%= @question.body %>')
  $('.question_box .files').html('<%= render 'shared/files', resource: @question %>')
  $('.question_box .links').html('<%= render @question.links %>')
  $('#edit-question').toggle()
<% end %>